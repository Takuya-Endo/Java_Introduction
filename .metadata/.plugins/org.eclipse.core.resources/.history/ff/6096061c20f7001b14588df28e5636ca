
public class exercise_4_1 {
	
	public static void main(String[] args) {
		
		int[] points;
		points = new int[4];
		points[0] = 1;
		points[1] = 2;
		points[2] = 3;
		points[3] = 4;
		
		double[] weights = new double[5];
		weights[0] = 1.1;
		weights[1] = 2.2;
		weights[2] = 3.3;
		weights[3] = 4.4;
		weights[4] = 5.5;
		
		
	}

}
