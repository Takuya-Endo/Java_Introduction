
public class exercise_3_1 {
	
	public static void main(String[] args) {
		
		int weight = 60;
		if (weight == 60) {
			System.out.println("①true");
		}
		
		int age1 = 10;
		int age2 = 20;
		if (((age1 + age2) * 2) >= 60) {
			System.out.println("②true");
		}
		
		int age1 = 10;
		int age2 = 20;
		if (((age1 + age2) * 2) >= 60) {
			System.out.println("②true");
		}
		
	}

}
