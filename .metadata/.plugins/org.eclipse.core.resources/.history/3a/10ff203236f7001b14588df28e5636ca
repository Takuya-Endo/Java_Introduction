
public class Main_6 {
	
	public static void main(String[] args) throws Exception {
		
		doWarusa();
		doTogame();
		callDeae();
		showMondokoro();
		
	}
	
	public static void doWarusa() {
		System.out.println("きなこでござる。食えませんがの。");
	}
	
	public static void doTogame() {
		System.out.println("この老いぼれの目はごまかせませんぞ。");
	}

}
